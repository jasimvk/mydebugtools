{
  "name": "mydebugtools",
  "version": "1.0.0",
  "description": "A comprehensive suite of debugging and development tools for web developers",
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_TELEMETRY_DISABLED=1 npm run generate-sitemap && next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf dist",
    "copy": "mkdir -p dist && cp -r chrome-extension/* dist/",
    "zip": "cd dist && zip -r ../mydebugtools.zip . -x \"*.DS_Store\" -x \"*.git*\"",
    "build:extension": "npm run clean && npm run copy && npm run zip",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate-sitemap": "node scripts/generate-sitemap.js"
  },
