{
  "name": "mydebugtools",
  "version": "1.0.0",
  "description": "A comprehensive suite of debugging and development tools for web developers",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf dist",
    "copy": "mkdir -p dist && cp -r chrome-extension/* dist/",
    "zip": "cd dist && zip -r ../mydebugtools.zip . -x \"*.DS_Store\" -x \"*.git*\"",
    "build:extension": "npm run clean && npm run copy && npm run zip"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@monaco-editor/react": "^4.7.0",
    "ajv": "^8.17.1",
    "csv-stringify": "^6.5.2",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.344.0",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "author": "",
  "license": "MIT"
}
